@model Sprotify.IDP.Models.RegisterViewModel

@{
    ViewBag.Title = "Sign up";
    Layout = "_SprotifyLayout";

    ViewBag.CssFile = "register.css";
    ViewBag.HtmlCssClasses = "layout-signup  js flexbox canvas no-touch hashchange rgba multiplebgs backgroundsize borderimage borderradius boxshadow textshadow opacity cssanimations cssgradients cssreflections csstransforms csstransforms3d csstransitions fontface video audio localstorage svg inlinesvg bgsizecover raf no-flexboxtweener";
    ViewBag.BodyCssClasses = "page-signup  m-uk l-en page-signup is-loggedout  reboot ";
}

<div class="wrap">
    <div class="sign-up">
        <div class="l-signup-header">
            <div class="container">
                <div class="l-box-content">
                    <a class="spotify-logo" href="/"></a>
                </div>
            </div>
        </div>
        <div class="l-signup-body">
            <div class="container">
                <div class="l-box-content">
                    <section class="register">
                        <div class="content">
                            <div><p></p></div>
                            <h2 class="center hdr-l">Sign up with your email address</h2>

                            <form asp-action="Register" asp-controller="Account" asp-antiforgery="true" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" novalidate="novalidate">
                                <div class="error has-error" asp-validation-summary="All">
                                </div>
                                <fieldset>
                                    <ul>
                                        <li>
                                            <label class="sr-only required" asp-for="Email"></label>
                                            <input asp-for="Email" placeholder="Email" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
                                        </li>

                                        <li>
                                            <label class="sr-only required" asp-for="Password"></label>
                                            <input asp-for="Password" placeholder="Password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                                        </li>

                                        <li>
                                            <label class="sr-only required" asp-for="ConfirmPassword"></label>
                                            <input asp-for="ConfirmPassword" placeholder="Confirm password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                                        </li>

                                        <li>
                                            <label class="sr-only required" asp-for="FirstName"></label>
                                            <input asp-for="FirstName" autocapitalize="off" autocorrect="off" autocomplete="off" placeholder="Your first name">
                                        </li>

                                        <li>
                                            <label class="sr-only required" asp-for="LastName"></label>
                                            <input asp-for="LastName" autocapitalize="off" autocorrect="off" autocomplete="off" placeholder="Your surname">
                                        </li>
                                    </ul>
                                </fieldset>
                                <button type="submit" id="register-button-email-submit" class="btn btn-green btn-block">
                                    Sign up
                                </button>
                            </form>
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </div>
</div>